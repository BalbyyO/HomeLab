# Timezone (used by NGINX Proxy Manager)
TZ=UTC

# ============================================
# Cloudflare DDNS (Optional)
# Uncomment the cloudflare-ddns service in
# docker-compose.yml to use these settings
# ============================================

# Your Cloudflare API token with DNS edit permissions
# Create one at: https://dash.cloudflare.com/profile/api-tokens
# CLOUDFLARE_API_TOKEN=your_token_here

# Comma-separated list of domains to update
# DOMAINS=example.com,proxy.example.com

# Whether to proxy traffic through Cloudflare (true/false)
# PROXIED=true

# IPv6 provider (none, cloudflare, local, or URL)
# IP6_PROVIDER=none
